{
  "name": "mocha-ctrf-json-reporter",
  "version": "0.0.10",
  "description": "A Mocha test reporter to create test results reports",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "mocha ./tests",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "all": "npm run lint && npm run format:check && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ctrf-io/mocha-ctrf-json-reporter.git"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "homepage": "https://ctrf.io",
  "files": [
    "dist/",
    "README.md"
  ],
  "author": "Matthew Poulton-White",
  "license": "MIT",
  "keywords": [
    "ctrf",
    "common-test-report-format",
    "test-reporting",
    "test-results",
    "testing",
    "json",
    "ci",
    "mocha",
    "mocha-reporter"
  ],
  "security": {
    "email": "security@ctrf.io",
    "url": "https://github.com/ctrf-io/.github/blob/main/SECURITY.md"
  },
  "devDependencies": {
    "@types/md5": "^2.3.6",
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.2.2",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.2",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-promise": "^7.2.1",
    "mocha": "^11.7.5",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0"
  },
  "dependencies": {
    "md5": "^2.3.0"
  }
}
